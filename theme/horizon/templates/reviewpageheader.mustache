{{#data}}
<div class="reviewpageheader quiz">
	<div class="d-flex justify-content-between reviewheader_content p-4">
		<div class="reviewheading">
			<h5>{{#str}}scorecard, quiz{{/str}}</h5>
			<p class="m-0"><span class="text-muted">{{#str}}testname, quiz{{/str}}</span> {{examname}}</p>
		</div>
		<div class="d-flex justify-content-center align-items-center">
			<a href="{{backtocourse}}"><button class="btn back_btn">{{#str}}backtocoursepage, quiz{{/str}}</button></a>
		</div>
		
	</div>
	<div class="px-4 pb-5 pt-4">
		<div class="review_card">
			<div class="row">
				<div class="col-md-2 col-4">
					<h5 class="review_data">{{reviewdata}}</h5>
					<p class="review_label">{{#str}}youhavescored, quiz{{/str}}</p>
				</div>
				<div class="col-md-2 col-4">
					<h5 class="review_data">{{timetaken}}</h5>
					<p class="review_label">{{#str}}timetakenbyuser, quiz{{/str}}</p>
				</div>
				<div class="col-md-2 col-4">
					<h5 class="review_data">{{answeredcorrect}}</h5>
					<p class="review_label">{{#str}}correctc, quiz{{/str}}</p>
				</div>
				<div class="col-md-2 col-4">
					<h5 class="review_data">{{answeredwrong}}</h5>
					<p class="review_label">{{#str}}wrongc, quiz{{/str}}</p>
				</div>
				<div class="col-md-2 col-4">
					<h5 class="review_data">{{unanswered}}</h5>
					<p class="review_label">{{#str}}unansweredc, quiz{{/str}}</p>
				</div>
				<div class="col-md-2 col-4">
					<h5 class="review_data">{{attempt}}</h5>
					<p class="review_label">{{#str}}attmptno, quiz{{/str}}</p>
				</div>
			</div>
		</div>
	</div>
</div>
{{/data}}