{{#dueactivity}}
<div class="calendar_events attempt_text">
    <div class="header_section">
        <div class="attempt_textimg">
            <!-- <i class="fa fa-sticky-note-o" aria-hidden="true"></i> -->
            <span class="icon"></span>

        </div>
        <div class="header_content align-items-center">
            <h5 class="m-0"><span>{{typeofevent}}</span>:<span>{{name}}</span></h5>
        </div>
    </div>
    <div class="row calendar_events_content">
        <div class="col-md-3 col-6">
            <div class="dateinfo">
                <div class="labels text-muted">Date</div>
                <div class="datainfo">{{#userdate}} {{timestart}}, {{#str}} strftimedaydatetime, block_notification {{/str}} {{/userdate}}</div>
            </div>
            <div class="timeinfo">
                <div class="labels text-muted">Time</div>
                <div class="datainfo">{{#userdate}} {{timestart}}, {{#str}} strftimetime12, core_langconfig {{/str}} {{/userdate}} - {{#timeduation}} {{#userdate}} {{timeend}}, {{#str}} strftimetime12, core_langconfig {{/str}} {{/userdate}} {{/timeduation}}{{^timeduation}}{{#str}} notavailable, block_notification {{/str}}{{/timeduation}}</div>
            </div>
        </div>
        <div class="col-md-3 col-6">
            <div class="duration">
                <div class="labels text-muted">Duration</div>
                <div class="datainfo">{{#duration}} {{duration}} {{/duration}} {{^duration}} - {{/duration}}</div>
            </div>
            <div class="typeinfo">
                <div class="labels text-muted">Type</div>
                <div class="datainfo">{{typeofevent}}</div>
            </div>  
        </div>
        <div class="col-md-3">
            <a href="{{url}}"><button class="text_btn">View</button></a>
        </div>
    </div> 
    
</div>
{{/dueactivity}}
{{^dueactivity}}
<div class="text-center">{{#str}}nodueactivities, block_notification{{/str}}</div>
{{/dueactivity}}
